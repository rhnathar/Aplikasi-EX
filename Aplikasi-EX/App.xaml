<Application x:Class="Aplikasi_EX.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Aplikasi_EX"
             xmlns:utils="clr-namespace:Aplikasi_EX.Utilities"
             StartupUri="/View/DetailProduct.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/inputBox.xaml"/>
                <ResourceDictionary Source="Styles/PageStyle.xaml"/>
                <ResourceDictionary Source="Styles/button.xaml"/>
                <ResourceDictionary Source="Styles/radioButton.xaml"/>
                <ResourceDictionary Source="Styles/imgClose.xaml"/>
                <ResourceDictionary Source="Utilities/DataTemplate.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <utils:ByteArrayToImageConverter x:Key="ByteArrayToImageConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
