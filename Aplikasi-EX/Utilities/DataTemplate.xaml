<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Aplikasi_EX.ViewModel"
                    xmlns:view="clr-namespace:Aplikasi_EX.View">

    <!-- Main Window -->
    <DataTemplate DataType="{x:Type vm:HomePageVM}">
        <view:HomePage/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AccountVM}">
        <view:Account/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AllProductsVM}">
        <view:AllProducts/>
    </DataTemplate>



</ResourceDictionary>