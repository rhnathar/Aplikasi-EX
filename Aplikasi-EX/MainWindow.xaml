<Window x:Class="Aplikasi_EX.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="User List" Height="450" Width="800">
    <Grid>
        <DataGrid ItemsSource="{Binding Users}" AutoGenerateColumns="True" />
        <Button Content="Load Users" Width="100" Height="30" Click="LoadUsersButton_Click" />
    </Grid>
</Window>